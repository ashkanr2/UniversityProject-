<form asp-action="Login" asp-controller="Account" method="post">
    <form id="form" class="m-t-30 m-b-30" asp-controller="Account" asp-action="Login" method="POST" role="form">
        <div class="form-group">
            <label for="email" class="sr-only">رایانامه</label>
            <div class="input-group round">
                <span class="input-group-addon">
                    <i class="icon-envelope"></i>
                </span>
                <input asp-for="UserName" class="form-control ltr text-left" type="email" required>
            </div><!-- /.input-group -->
        </div><!-- /.form-group -->
        <div class="form-group">
            <label for="password" class="sr-only">رمز عبور</label>
            <div class="input-group round">
                <span class="input-group-addon">
                    <i class="icon-key"></i>
                </span>
                <input asp-for="Password" id="password" class="form-control ltr text-left" type="password" minlength="5" required>
            </div><!-- /.input-group -->
        </div><!-- /.form-group -->
        <p>
            <button class="btn btn-info btn-round btn-block" type="submit"> ورود </button>

        </p>
    </form>
</form>

 
